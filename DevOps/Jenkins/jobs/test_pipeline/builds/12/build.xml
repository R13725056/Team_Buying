    <script>pipeline {
    agent any

    tools {
        nodejs &quot;NodeJS&quot;  // 使用你在 Jenkins 中配置的 NodeJS 名稱
    }

    stages {
        stage(&apos;Checkout&apos;) {
            steps {
                // 檢出 Git 資料庫中的代碼
                git &apos;https://github.com/R13725056/Team_Buying.git&apos;
            }
        }

        stage(&apos;Install Dependencies&apos;) {
            steps {
                dir(&apos;front-end/src&apos;) { // 進入正確的目錄
                    // 安裝 npm 依賴
                    sh &apos;npm install&apos;
                }
            }
        }

        stage(&apos;Run Tests&apos;) {
            steps {
                dir(&apos;front-end/src&apos;) { // 進入正確的目錄
                    // 執行測試
                    sh &apos;npm test&apos;
                }
            }
        }

        stage(&apos;Build&apos;) {
            steps {
                dir(&apos;front-end/src&apos;) { // 進入正確的目錄
                    // 建構應用程序
                    sh &apos;npm run build&apos;
                }
            }
        }
    }

    post {
        always {
            // 清理工作區以節省磁盤空間
            cleanWs()
        }
        success {
            // 發送通知或做其他處理
            echo &apos;Build and test completed successfully.&apos;
        }
        failure {
            // 處理失敗的情況，例如發送失敗通知
            echo &apos;Build or test failed.&apos;
        }
    }
}
</script>
